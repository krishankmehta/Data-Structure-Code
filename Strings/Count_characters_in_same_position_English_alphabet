//To count characters in same position as in English alphabet





#include<iostream>


using namespace std;


int main()
{
    int count=0;
    string x ="abcdgfg";
    for(int i=0;i<x.length();i++)
    {
        if(i==(x[i] - 'a')||i==(x[i] - 'A'))
        {
            ++count;
        }
    }
    cout<<count<<endl;
}
